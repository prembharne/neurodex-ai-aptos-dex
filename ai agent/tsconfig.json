{
  "compilerOptions": {
    "target": "ES2022",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
  "jsx": "react-jsx",
  "types": ["node"]
  },
  "include": ["src/**/*", "server/**/*"],
  "exclude": ["node_modules", "dist"]
}
